{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.gif",
          "**/*.jpeg",
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-Security",
      "breadcrumb_path": "/security/breadcrumb/toc.json",
      "feedback_system": "Standard",
      "feedback_help_link_type": "get-help-at-qna"
    },
    "fileMetadata": {
      "titleSuffix": {
        "*.md": "Security",
        "*.yml": "Security",
        "privileged-access-workstations/*.md": "Privileged access",
        "privileged-access-workstations/*.yml": "Privileged access"
      },
      "feedback_system": {
        "privileged-access-workstations/**/*.md": "None",
        "ai-red-team/**/*.md": "None"
      }
    },
    "template": [],
    "dest": "security-docs"
  }
}